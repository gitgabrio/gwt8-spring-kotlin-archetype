#set( $symbol_pound = '#' )
#set( $symbol_dollar = '$' )
#set( $symbol_escape = '\' )
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:b='urn:import:org.gwtbootstrap3.client.ui'
             xmlns:login='urn:import:${package}.client.view.login'>
    <g:HTMLPanel styleName="login-view">
        <b:Container fluid="true" styleName="login-view">
            <b:Row>
                <b:Column size="MD_4">
                    <login:LoginView ui:field="fakeLoginView"/>
                </b:Column>
            </b:Row>
        </b:Container>
    </g:HTMLPanel>
</ui:UiBinder>